//********************** SELECTION SORT*****************************//
// TIME COMPLEXITY O(N^2)
// SPACE COMPLEXITY O(1)

 void selectionSort(int arr[], int n)
    {
     for(int i=0;i<n;i++){
         for(int j=i+1;j<n;j++){
             if(arr[j]<arr[i]){
                int temp=arr[j];
                arr[j]=arr[i];
                arr[i]=temp;
             }
         }
     }
  }
